{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": "eastus",
            "allowedValues": [
                "East US",
                "eastus",
                "East US 2",
                "North Central US",
                "South Central US",
                "West US",
                "North Europe",
                "West Europe",
                "East Asia",
                "Southeast Asia",
                "Japan East",
                "Japan West",
                "Australia East",
                "Australia Southeast"
            ],
            "metadata": {
                "description": "The location where all azure resources will be deployed."
            }
        },
        "sqlDWServerName": {
            "type": "string",
            "metadata": {
                "description": "SQL Datawarehouse Server Name, should be a unique name"
            }
        },
        "sqlDWDBAdminName": {
            "type": "string",
            "metadata": {
                "description": "SQL Datawarehouse Admin Name"
            }
        },
        "sqlDWAdminPassword": {
            "type": "securestring",
            "metadata": {
                "description": "SQL Datawarehouse Admin password. Ex: Testadmin@123"
            }
        },
        "sqlDWDBName": {
            "type": "string",
            "defaultValue": "testdwdb",
            "metadata": {
                "description": "SQL Datawarehouse Database Name"
            }
        },
        "serviceLevelObjective": {
            "type": "string",
            "defaultValue": "DW100",
            "allowedValues": [
                "DW100",
                "DW100",
                "DW200",
                "DW300",
                "DW400",
                "DW500",
                "DW600",
                "DW1000",
                "DW1200",
                "DW1500",
                "DW2000",
                "DW3000",
                "DW6000"
            ]
        },
        "startIpAddress": {
            "type": "string",
            "defaultValue": "0.0.0.0"
        },
        "endIpAddress": {
            "type": "string",
            "defaultValue": "255.255.255.255"
        },
        "jobId": {
            "type": "string",
            "defaultValue": "5f32ff4e-a5c5-47af-8b70-981c896a9163",
            "metadata": {
                "description": "Unique GUID"
            }
        },
        "accountName": {
            "type": "string",
            "metadata": {
                "description": "Name of the automation account. It should be unique"
            }
        },
        "credentialName": {
            "type": "string",
            "metadata": {
                "description": "Name for credentials"
            }
        },
        "runbookName": {
            "type": "string",
            "defaultValue": "createtable",
            "metadata": {
                "description": "Name of the runbook"
            }
        },
        "dataFactoryName": {
            "type": "string",
            "metadata": {
                "description": "Name of the data factory. It must be globally unique"
            }
        },
        "start": {
            "type": "string",
            "metadata": {
                "description": "Start time of the data slice. ex: 2014-06-01T18:00:00"
            }
        },
        "end": {
            "type": "string",
            "metadata": {
                "description": "Start time of the data slice. ex: 2014-06-01T18:00:00"
            }
        },
        "tableName": {
            "type": "string",
            "defaultValue": "bi9"
        },
        "inputFolderPath": {
            "type": "string",
            "defaultValue": "adfgetstarted/inputdata"
        },
        "outputFolderPath": {
            "type": "string",
            "defaultValue": "adfgetstarted/partitioneddata"
        },
        "tag": {
            "type": "object",
            "defaultValue": {
                "value1": "informatica"
            }
        },
        "vmName": {
            "type": "string",
            "defaultValue": "informatica",
            "metadata": {
                "description": " Virtual machine name"
            }
        },
        "adminUsername": {
            "type": "string",
            "metadata": {
                "description": "adminusername to login into vm"
            }
        },
        "adminPassword": {
            "type": "securestring",
            "metadata": {
                "description": " adminpassword for vm"
            }
        },
        "storageAccountName": {
            "type": "string",
            "defaultValue": "infosa",
            "metadata": {
                "description": "Storage Account name"
            }
        },
        "adfstorageAccountName": {
            "type": "string",
            "defaultValue": "adfteststore234"
        },
        "publicIPdnsPrefix": {
            "type": "string",
            "metadata": {
                "description": " public IP dns prefix"
            }
        },
        "fileUris": {
            "type": "string",
            "defaultValue": "https://raw.githubusercontent.com/sysgain/informatica-p2p/master/scripts/ICExtentionScript.ps1"
        },
        "arguments": {
            "type": "string",
            "metadata": {
                "description": "pass in the username and password,use doublequotes around the arguments with spaces"
            }
        },
       "script": {
	 "type": "string",
      	"defaultValue": ""
    }
    },
    "variables": {
        "storageAccName": "[concat(parameters('storageAccountName'),uniqueString(resourceGroup().id))]",
        "adfstorageAccName": "[concat(parameters('adfstorageAccountName'),uniqueString(resourceGroup().id))]",
        "pubicIPAddressName": "[concat(parameters('vmName'),'-publicIp')]",
        "vnetName": "[concat(parameters('vmName'),'-vnet')]",
        "networkInterfaceName": "[concat(parameters('vmName'),'-nic')]",
        "networkSecurityGroupsName": "[concat(parameters('vmName'),'-nsg')]",
        "subnet1Name": "subnet1",
        "vnetAddressPrefix": "10.2.0.0/16",
        "subnet1Prefix": "10.2.0.0/24",
        "storageAccountType": "Standard_LRS",
        "vmSize": "Standard_A2",
        "storageContainer": "vhds",
        "imagePublisher": "informatica-cloud",
        "imageOffer": "informatica-cloud",
        "imageVersion": "latest",
        "imageSKU": "informatica_cloud_secure_agent_32_bit",
        "subnetRef": "[concat(resourceId('Microsoft.Network/virtualNetworks', variables('vnetName')), '/subnets/',variables('subnet1Name'))]",
        "storageApiVersion": "2015-06-15",
        "networkApiVersion": "2015-06-15",
        "computeApiVersion": "2015-06-15",
        "armBaseUrl": "https://raw.githubusercontent.com/sysgain/informatica-p2p/master/nested/",
        "armComponentUrl": "https://raw.githubusercontent.com/sysgain/arm-components/",
        "publicIpAddressUrl": "[concat(variables('armBaseUrl'),'public-ip.json')]",
        "storageAccountUrl": "[concat(variables('armBaseUrl'),'storage.json')]",
        "virtualNetworkUrl": "[concat(variables('armBaseUrl'),'vnet-subnet.json')]",
        "networkInterfaceUrl": "[concat(variables('armBaseUrl'),'network-interface.json')]",
        "networkSecurityGroupUrl": "[concat(variables('armBaseUrl'),'info-nsg.json')]",
        "virtualMachineUrl": "[concat(variables('armBaseUrl'),'virtual-machine-with-plan.json')]",
        "virtualMachineExtUrl": "[concat(variables('armBaseUrl'),'info-csa-extension.json')]",
        "sqlDataWareHouseSetupURL": "[concat(variables('armBaseUrl'),'sqldatawarehouse.json')]",
        "createTableAutomationSetupURL": "[concat(variables('armBaseUrl'),'createtableautomationjob.json')]",
        "adfSetupURL": "[concat(variables('armBaseUrl'),'azuredatafactory.json')]",
        "apiVersion": "2015-10-01",
        "storageLinkedServiceName": "AzureStorageLinkedService",
        "hdInsightOnDemandLinkedServiceName": "HDInsightOnDemandLinkedService",
        "azureSqlDWLinkedServiceName": "AzureSqlDWLinkedService",
        "blobInputDataset": "AzureBlobInput",
        "blobOutputDataset": "AzureBlobOutput",
        "sqlDWOutputDataset": "AzureSqlDWOutput",
        "azureSqlDWLinkedServiceConnectionString": "[concat('Server=tcp:',parameters('sqlDWServerName'),'.database.windows.net,1433;Database=', parameters('sqlDWDBName'), ';Trusted_Connection=False;User ID=',parameters('sqlDWDBAdminName'),'@',parameters('sqlDWServerName'),';Password=',parameters('sqlDWAdminPassword'),';Connection Timeout=30;Encrypt=True')]",
        "clusterSize": 1,
        "version": "3.2",
        "sql-version":"12.0",
        "timeToLive": "00:45:00",
        "frequency": "Hour",
        "interval": 1,
        "writeBatchSize": 0,
        "writeBatchTimeout": "00:00:00",
        "timeout": "01:00:00",
        "automationjoblocation": "South Central US",
        "baseURL": "https://susehpcnc.blob.core.windows.net/test/",
        "scriptUri": "[concat(variables('baseURL'),'createTable.ps1')]",
        "runbookDescription": "Create a Database Table in User provided Datawarehouse",
        "sku": "Basic",
        "sqldwlocation":"South Central US",
        "sql-api-version": "2014-04-01",
        "sqldb-api-version": "2015-05-01-preview",
        "sqlfirewallrules-api-version": "2014-04-01",
        "collation": "SQL_Latin1_General_CP1_CI_AS",
        "maxSizeBytes": "10995116277760"
    },
    "resources": [
        {
            "name": "publicIpAddressDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('publicIpAddressUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "networkApiVersion": {
                        "value": "[variables('networkApiVersion')]"
                    },
                    "publicIPAddressName": {
                        "value": "[variables('pubicIPAddressName')]"
                    },
                    "publicIPdnsPrefix": {
                        "value": "[parameters('publicIPdnsPrefix')]"
                    },
                    "tag": {
                        "value": {
                            "key1": "Public IP Address",
                            "value1": "[parameters('tag').value1]"
                        }
                    }
                }
            }
        },
        {
            "name": "storageAccountDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('storageAccountUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "storageApiVersion": {
                        "value": "[variables('storageApiVersion')]"
                    },
                    "storageAccountType": {
                        "value": "[variables('storageAccountType')]"
                    },
                    "storageAccountName": {
                        "value": "[variables('storageAccName')]"
                    },
                    "tag": {
                        "value": {
                            "key1": "Storage Account",
                            "value1": "[parameters('tag').value1]"
                        }
                    }
                }
            }
        },
        {
            "name": "adfstorageAccountDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('storageAccountUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "storageApiVersion": {
                        "value": "[variables('storageApiVersion')]"
                    },
                    "storageAccountType": {
                        "value": "[variables('storageAccountType')]"
                    },
                    "storageAccountName": {
                        "value": "[parameters('adfstorageAccountName')]"
                    },
                    "tag": {
                        "value": {
                            "key1": "Storage Account",
                            "value1": "[parameters('tag').value1]"
                        }
                    }
                }
            }
        },
        {
            "name": "virtualNetworkDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('virtualNetworkUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "networkApiVersion": {
                        "value": "[variables('networkApiVersion')]"
                    },
                    "vnetName": {
                        "value": "[variables('vnetName')]"
                    },
                    "subnet1Name": {
                        "value": "[variables('subnet1Name')]"
                    },
                    "vnetAddressPrefix": {
                        "value": "[variables('vnetAddressPrefix')]"
                    },
                    "subnet1Prefix": {
                        "value": "[variables('subnet1Prefix')]"
                    },
                    "tag": {
                        "value": {
                            "key1": "Virtual Network",
                            "value1": "[parameters('tag').value1]"
                        }
                    }
                }
            }
        },
        {
            "name": "networkInterfaceDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "dependsOn": [
                "virtualNetworkDeploy",
                "networkSecurityGroupDeploy",
                "publicIpAddressDeploy"
            ],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('networkInterfaceUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "networkApiVersion": {
                        "value": "[variables('networkApiVersion')]"
                    },
                    "networkInterfaceName": {
                        "value": "[variables('networkInterfaceName')]"
                    },
                    "networkSecurityGroupName": {
                        "value": "[variables('networkSecurityGroupsName')]"
                    },
                    "publicIPAddressName": {
                        "value": "[variables('pubicIPAddressName')]"
                    },
                    "subnetRef": {
                        "value": "[variables('subnetRef')]"
                    },
                    "tag": {
                        "value": {
                            "key1": "Network Interface",
                            "value1": "[parameters('tag').value1]"
                        }
                    }
                }
            }
        },
        {
            "name": "networkSecurityGroupDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('networkSecurityGroupUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "networkApiVersion": {
                        "value": "[variables('networkApiVersion')]"
                    },
                    "networkSecurityGroupsName": {
                        "value": "[variables('networkSecurityGroupsName')]"
                    },
                    "tag": {
                        "value": {
                            "key1": "Network Security Group",
                            "value1": "[parameters('tag').value1]"
                        }
                    }
                }
            }
        },
        {
            "name": "virtualMachineDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "dependsOn": [
                "storageAccountDeploy",
                "networkInterfaceDeploy"
            ],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('virtualMachineUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "computeApiVersion": {
                        "value": "[variables('computeApiVersion')]"
                    },
                    "vmName": {
                        "value": "[parameters('vmName')]"
                    },
                    "storageAccountName": {
                        "value": "[variables('storageAccName')]"
                    },
                    "vmStorageAccountContainerName": {
                        "value": "[variables('storageContainer')]"
                    },
                    "vmSize": {
                        "value": "[variables('vmSize')]"
                    },
                    "imagePublisher": {
                        "value": "[variables('imagePublisher')]"
                    },
                    "imageOffer": {
                        "value": "[variables('imageOffer')]"
                    },
                    "imageVersion": {
                        "value": "[variables('imageVersion')]"
                    },
                    "imageSKU": {
                        "value": "[variables('imageSKU')]"
                    },
                    "adminUsername": {
                        "value": "[parameters('adminUsername')]"
                    },
                    "adminPassword": {
                        "value": "[parameters('adminPassword')]"
                    },
                    "networkInterfaceName": {
                        "value": "[variables('networkInterfaceName')]"
                    },
                    "tag": {
                        "value": {
                            "key1": "Virtual Machine",
                            "value1": "[parameters('tag').value1]"
                        }
                    }
                }
            }
        },
        {
            "name": "virtualMachineExtDeploy",
            "dependsOn": [
                "virtualMachineDeploy"
            ],
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('virtualMachineExtUrl')]"
                },
                "parameters": {
                    "vmName": {
                        "value": "[parameters('vmName')]"
                    },
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "fileUris": {
                        "value": "[parameters('fileUris')]"
                    },
                    "arguments": {
                        "value": "[parameters('arguments')]"
                    }
                }
            }
        },
        {
            "name": "sqlDataWarehoueSetup",
            "apiVersion": "2015-01-01",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('sqlDataWareHouseSetupURL')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[variables('sqldwlocation')]"
                    },
                    "sqlDWServerName": {
                        "value": "[parameters('sqlDWServerName')]"
                    },
                    "sqlDWDBAdminName": {
                        "value": "[parameters('sqlDWDBAdminName')]"
                    },
                    "sqlDWAdminPassword": {
                        "value": "[parameters('sqlDWAdminPassword')]"
                    },
                    "sqlDWDBName": {
                        "value": "[parameters('sqlDWDBName')]"
                    },
                    "serviceLevelObjective": {
                        "value": "[parameters('serviceLevelObjective')]"
                    },
                    "startIpAddress": {
                        "value": "[parameters('startIpAddress')]"
                    },
                    "endIpAddress": {
                        "value": "[parameters('endIpAddress')]"
                    },
                    "sql-api-version": {
                        "value": "[variables('sql-api-version')]"
                    },
                    "sqldb-api-version": {
                        "value": "[variables('sqldb-api-version')]"
                    },
                    "sqlfirewallrules-api-version": {
                        "value": "[variables('sqlfirewallrules-api-version')]"
                    },
                    "collation": {
                        "value": "[variables('collation')]"
                    },
                    "maxSizeBytes": {
                        "value": "[variables('maxSizeBytes')]"
                    },
                    "version": {
                        "value": "[variables('sql-version')]"
                    }
                }
            }
        },
        {
            "name": "createTableAutomationSetup",
            "apiVersion": "2015-01-01",
            "type": "Microsoft.Resources/deployments",
            "dependsOn": [
                "sqlDataWarehoueSetup"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('createTableAutomationSetupURL')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "sqlDWServerName": {
                        "value": "[parameters('sqlDWServerName')]"
                    },
                    "sqlDWDBAdminName": {
                        "value": "[parameters('sqlDWDBAdminName')]"
                    },
                    "sqlDWAdminPassword": {
                        "value": "[parameters('sqlDWAdminPassword')]"
                    },
                    "sqlDWDBName": {
                        "value": "[parameters('sqlDWDBName')]"
                    },
                    "jobId": {
                        "value": "[parameters('jobId')]"
                    },
                    "accountName": {
                        "value": "[parameters('accountName')]"
                    },
                    "credentialName": {
                        "value": "[parameters('credentialName')]"
                    },
                    "runbookName": {
                        "value": "[parameters('runbookName')]"
                    },
                    "location": {
                        "value": "South Central US"
                    },
                    "baseURL": {
                        "value": "https://susehpcnc.blob.core.windows.net/test/"
                    },
                    "scriptUri": {
                        "value": "[concat(variables('baseURL'),'createTable.ps1')]"
                    },
                    "runbookDescription": {
                        "value": "Create a Database Table in User provided Datawarehouse"
                    },
                    "sku": {
                        "value": "Basic"
                    }
                }
            }
        },
        {
            "name": "dataFactorySetup",
            "apiVersion": "2015-01-01",
            "type": "Microsoft.Resources/deployments",
            "dependsOn": [
                "sqlDataWarehoueSetup"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('adfSetupURL')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "sqlDWServerName": {
                        "value": "[parameters('sqlDWServerName')]"
                    },
                    "sqlDWDBAdminName": {
                        "value": "[parameters('sqlDWDBAdminName')]"
                    },
                    "sqlDWAdminPassword": {
                        "value": "[parameters('sqlDWAdminPassword')]"
                    },
                    "sqlDWDBName": {
                        "value": "[parameters('sqlDWDBName')]"
                    },
                    "dataFactoryName": {
                        "value": "[parameters('dataFactoryName')]"
                    },
                    "start": {
                        "value": "[parameters('start')]"
                    },
                    "end": {
                        "value": "[parameters('end')]"
                    },
                    "tableName": {
                        "value": "[parameters('tableName')]"
                    },
                    "inputFolderPath": {
                        "value": "[parameters('inputFolderPath')]"
                    },
                    "adfstorageAccountName": {
                        "value": "[parameters('adfstorageAccountName')]"
                    },
                    "outputFolderPath": {
                        "value": "[parameters('outputFolderPath')]"
                    },
                  "apiVersion": {
			             "value": "[variables('apiVersion')]"
                    },
                    "storageLinkedServiceName": {
                         "value": "[variables('storageLinkedServiceName')]"
                    },
                    "hdInsightOnDemandLinkedServiceName": {
		                 "value": "[variables('hdInsightOnDemandLinkedServiceName')]"
                     },
                    "azureSqlDWLinkedServiceName": {
                         "value": "[variables('azureSqlDWLinkedServiceName')]"
                     },
                     "blobInputDataset": {
                         "value": "[variables('blobInputDataset')]"
                     },
                     "blobOutputDataset": {
                         "value": "[variables('blobOutputDataset')]"
                      },
                    "sqlDWOutputDataset": {
                          "value": "[variables('sqlDWOutputDataset')]"
                     },
                     "azureSqlDWLinkedServiceConnectionString": {
                          "value": "[variables('azureSqlDWLinkedServiceConnectionString')]"
                     },
                     "clusterSize": {
	                    	"value": "[variables('clusterSize')]"
                      },
                     "version": {
	                    	"value": "[variables('version')]"
                      },
                      "timeToLive": {
		                    "value": "[variables('timeToLive')]"
                     },
                    "frequency": {
	                    	"value": "[variables('frequency')]"
                      },
                    "interval": {
		                     "value": "[variables('interval')]"
                     },
                     "writeBatchSize": {
                               "value": "[variables('writeBatchSize')]"
                      },
                      "writeBatchTimeout": {
		                    "value": "[variables('writeBatchTimeout')]"
                      },
                      "timeout": {
	                    	"value": "[variables('timeout')]"
                      },
                      "script": {
	                    	"value": "[parameters('script')]"
                      }
        
                }
            }
        }
    ]
}
